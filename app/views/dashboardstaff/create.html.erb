<h1><u>Update Status of Document</u></h1>
<%= simple_form_for(@assignment) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :accept %>
    <%= f.input :location %>
    <%= f.input :status %>
    <%= f.input :datecomplete %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
