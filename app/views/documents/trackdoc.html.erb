<%= form_tag(documents_trackdoc_path, :method => "get", id: "search-form") do %>
<%= text_field_tag :search, params[:search], placeholder: "Search Posts" %>
<%= submit_tag "Search" %>
<% end %>

<h1>Documents Details</h1>


<table class="table table-striped table-hover ">
<tr>
	<th>Document </th>
	<th>Type of Document</th>
	<th>Level</th>
	<th>Staff Handle</th>
	<th>Location</th>
</tr>
	<% @doctask.each do |dt| %>
<tr>
	<td><%= dt.document.try(:serialno) %></td>
	<td><%= dt.task.form.try(:formname) %></td>	
	<td><%= dt.task.try(:level) %></td>
	<td><%= dt.task.user.try(:username)%></td>
	<td><%= dt.task.assignments.try(:location)%></td>
</tr>
<%end %>
</table>

