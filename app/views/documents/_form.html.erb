
<%= simple_form_for document do |f| %>

  <div class="form-input">
    <%= f.input :title %>
    <%= f.input :attachment%>
    <%= f.input :desc ,:label => "Description" %>
    <%= f.input :datesubmit, as: :datetime ,:label => "Date and Time Submit"%>
    <%= f.association :form, :label_method => :formname, :value_method => :id ,:label => "Choose Form " %>
    <%= f.button :submit , :class => "btn btn-success" %>
  </div>
<% end %>
