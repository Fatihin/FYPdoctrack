<%= simple_form_for(@form) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :formid ,:label => "Form ID" %>
    <%= f.input :formname ,:label => "Form name"%>
    <%= f.input :attachment  %>
  </div>

  <div class="form-actions">
    <%= f.button :submit , :class => "btn btn-success" %>
  </div>
<% end %>
