<h1>Report</h1>

<div class ="row">
	<div class="col-xs-6">
    <h3>Total Document Submit </h3>
    <%= pie_chart @document.group(:formname).count %>
  </div>
  
  <div class="col-xs-6">
    <h3>Task complete by day</h3>
    <%= line_chart @assignment.group("DATE(datecomplete)").count, discrete: true %>
  </div>

  <div class="col-xs-6">
    <h3>Total task by staff</h3>
    <%= column_chart @t.group(:username).count, discrete: true %>
  </div>

 
</div>